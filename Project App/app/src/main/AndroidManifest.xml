<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectApplication">
        <activity android:name=".PasswordStrengthhelp"
            android:parentActivityName=".PasswordStrength"
            ></activity>
        <activity
            android:name=".QuizInfo"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Quiz"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PasswordStrength"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PasswordGenerator"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>